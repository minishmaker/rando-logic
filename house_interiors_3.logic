area HouseInteriors3:
  room Bakery:
    node Main:
      door West
      npc Wheaton
      npc Pita
    node MainMinish:
      door EastMinish
      door NorthEastMinish
      door NorthWestMinish
  room BorlovFront:
    node Main:
      target North
      door South
      npc Borlov
    node Game:
      warp North
    Main -> North: spend10
  room BorlovBack:
    node Main:
      door Door
      npc Borlov
  room Cafe:
    node Main:
      door South
      set BookcaseMoved
      npc Baris
      npc "Hurdy-Gurdy Man"
      npc Chai
      npc Mama
      npc Breve
      npc Gepper
    node FlipPot:
      set PortalFlipped
    Main -> FlipPot: item Cane
    node MainMinish:
      door SouthMinish
      door EastMinish
      target RoofHole
