    MainMinish -> Main: ( flag PortalFlipped & helper CanGrow )
    node PushBookcase:
      set BookcaseMoved
    MainMinish -> PushBookcase: item PowerBracelets
    node MinishLedge:
      door MinishLedge
    MainMinish -> MinishLedge: flag BookcaseMoved 
    MinishLedge -> MainMinish: flag BookcaseMoved
  room Carlov:
    node Main:
      npc Carlov
      door South
  room FigurineHouse:
    node West:
      door West
    node East:
      chest Chest1
      chest Chest2
      chest Chest3
      chest HP
      target East
    node EastExit:
      warp East
    East -> EastExit: item CarlovMedal
  room Rem:
    node Main:
      door East
    node Wake:
      set RemReawakened
    Main -> Wake: ( flag HyruleField.WestWoodsNorth.Sign & item WakeUpMushroom )
    node Woken:
      chest Rem
      npc Rem
    Main -> Woken: flag RemReawakened
    node MainMinish:
      door MinishLadder
    Main -> MainMinish: helper CanTurnMinish
    MainMinish -> Main: helper CanGrow
  room Simon:
    node Main:
      door East
      target Bed
      npc Simon
    node Minigame:
      warp Bed
    Main -> Minigame: spend10
  room Shop:
    node Back:
      chest BackItem
    node BackMinish:
      door MinishRafters
    Back -> BackMinish: item CanTurnMinish
    BackMinish -> Back: item CanGrow
    node Main:
      door South
      npc Stockwell
    node FirstItem:
      chest Item1
    node SecondItem:
      chest Item2
    node ThirdItem:
      chest Item3
    Main -> FirstItem: spend80
    FirstItem -> SecondItem: spend300
    Main -> ThirdItem: ( spend600 & item Bow )
